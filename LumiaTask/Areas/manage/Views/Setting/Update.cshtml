@model Setting
<form method="post">
    <div class="mb-3">
        <label asp-for="Value" class="form-label">@Model.Key</label>
        <input asp-for="Value" class="form-control" >
        <span asp-validation-for="Value" class="text-danger"></span>
    </div>
    
    <button type="submit" class="btn btn-primary">Submit</button>
</form>